<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorMain">

    <Button
        android:id="@+id/home_button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Inicio"
        app:layout_constraintEnd_toStartOf="@+id/anadir_diario_button3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="16dp"/>

    <Button
        android:id="@+id/anadir_diario_button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Añadir diario"
        app:layout_constraintEnd_toStartOf="@+id/resultados_button2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/home_button3"
        tools:layout_editor_absoluteY="16dp"/>

    <Button
        android:id="@+id/resultados_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Resultados"
        app:layout_constraintEnd_toStartOf="@+id/ayuda_button3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/anadir_diario_button3"
        tools:layout_editor_absoluteY="16dp"/>

    <Button
        android:id="@+id/ayuda_button3"
        android:layout_width="91dp"
        android:layout_height="43dp"
        android:text="Ayuda"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/resultados_button2"
        tools:layout_editor_absoluteY="21dp"/>

    <EditText
        android:id="@+id/editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="96dp"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="110dp"
        android:layout_height="48dp"
        android:text="Fecha:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="155dp"/>

    <EditText
        android:id="@+id/editText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="155dp"/>

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Exportar"
        tools:layout_editor_absoluteX="297dp"
        tools:layout_editor_absoluteY="221dp"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Síntoma"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="318dp"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Resultado"
        tools:layout_editor_absoluteX="229dp"
        tools:layout_editor_absoluteY="318dp"/>

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Buscar Síntoma"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="261dp"/>

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Buscar Resultado"
        tools:layout_editor_absoluteX="229dp"
        tools:layout_editor_absoluteY="260dp"/>

</android.support.constraint.ConstraintLayout>